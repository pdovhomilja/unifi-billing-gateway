DATABASE_URL="postgresql://unifi-billing:5432/unifi-billing?schema=public"

#UNIFI
UNIFI_CONTROLLER_URL="https://your-unifi-controller-url:8443"
UNIFI_USERNAME="SomeUsername"
UNIFI_PASSWORD="SomePassword"
UNIFI_SITE="site-name" # default is "default" - https://192.168.1.94:8443/manage/xbosenxl/insights/hotspot/vouchers - then "xbosenxl" is the site name

NODE_TLS_REJECT_UNAUTHORIZED="0"

